    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.1.18",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "lodash": "^4.15.0",
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-google-maps": "^4.11.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.0.4",
    "redux-promise": "^0.5.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  }
